{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react"
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
    ],
    "exclude": [
        "node_modules"
    ]
}